{
  "opts": {
    "template": "node_modules/jaguarjs-jsdoc",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "tutorials": "./tutorials"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [ "lib", "README.md" ],
    "exclude": [],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [ "plugins/markdown" ],
  "templates": {
      "cleverLinks": true,
      "monospaceLinks": true,
      "default": {
        "outputSourceFiles" : true
      },
      "applicationName": "Shop Core",
      "disqus": "",
      "googleAnalytics": "",
      "openGraph": {
          "title": "",
          "type": "website",
          "image": "",
          "site_name": "",
          "url": ""
      },
      "meta": {
          "title": "",
          "description": "",
          "keyword": ""
      },
      "linenums": true
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true,
    "tags": [ "examples" ]
  }
}
